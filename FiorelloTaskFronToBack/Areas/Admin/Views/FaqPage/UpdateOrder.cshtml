@using FiorelloTaskFronToBack.Areas.Admin.ViewModels.FaqPage;
@model FaqPageUpdateOrderViewModel;

<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">ADD Category</h4>
            <div class="row">
                <form method="post">
                    <div class="form-group">
                        <label asp-for="@Model.Order"></label>
                        <input asp-for="@Model.Order" class="form-control">
                        <span asp-validation-for="@Model.Order"></span>
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                    <a class="btn btn-light" asp-action="index" asp-controller="faqpage" asp-area="admin">Cancel</a>
                </form>
            </div>
        </div>
    </div>
</div>